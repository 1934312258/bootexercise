<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kevin.mapper.OrderInfoMapper">
    <insert id="saveOrderInfo" parameterType="com.kevin.entity.OrderInfo">
        insert into order_info(order_no,create_time,update_time,user_name,money,product_no)
        values(#{orderNo},#{createTime},#{updateTime},#{userName},#{money},#{productNo})
    </insert>

</mapper>